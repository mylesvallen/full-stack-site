<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
		<!-- Optional theme -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
<link rel="stylesheet" href="/css/style2.css" type="text/css" />


<div class="container">
	<div class="row">
		<div class="col-xs-12 text-center" id="top-bar">
			<h1>Tenant's Ratings</h1>
		</div>
		<div class="col-xs-6 text-center" class="task">
			<div class="row">
            <ul class="ratingsData">
			  {{#each ratings_data}}
                {{#if ratings_data}} 
                    <li>{{{ratings}}}
                        <div class="ratings-table"> 
                            <div class="image">
                                <img src = "/assets/image/icon-image.png" alt="icon"/>
                            </div>
                            <div class="ratins-static">
                                <span class="rating-static rating-40"></span>
                                <span class="rating-static rating-20"></span>
                                <span class="rating-static rating-40"></span>
                                <span class="rating-static rating-30"></span>
                                <span class="rating-static rating-50"></span>
                                <span class="rating-static rating-30"></span> 
                                <span class="rating-static rating-30"></span>
                                <span class="rating-static rating-40"></span> 
                                <span class="rating-static rating-20"></span>
                                <span class="rating-static rating-50"></span> 
                                <span class="rating-static rating-30"></span>          
                            </div>
                        <h4>{{property_name}} <span class="properties">({{properties}})</span></h4>
                        <h4>{{tenant_name}}  <span class="tenants">({{tenants}})</span></h4> 
                        <h4>{{tenant_comment}}  <span class="tenants">({{tenant_comment}})</span></h4> 
                            {{this.Date}}
                </li>
              {{/each}}
            </ul>
			</div>          
<div class="container">
	<div class="row">
		<div class="col-xs-12 text-center enter_section">
			<form action="/create" method="POST">
				<input type="text" class="form-control" name="ratings" value="" id="enter_text">
				<button type="submit" class="btn btn-primary" id="text-enter-button">Submit</button>
			</form>
			<p></p>
		</div>
	</div>
</div>